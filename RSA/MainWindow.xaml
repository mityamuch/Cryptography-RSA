<Window x:Class="RSA.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RSA"
        xmlns:ViewModels="clr-namespace:RSA.Client.ViewModels"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.DataContext>
        <ViewModels:MainWindowViewModel/>
    </Window.DataContext>
    <Grid>
        <TextBlock x:Name="test" FontSize="50"></TextBlock>
        <Button Height="40" Width="100" VerticalAlignment="Bottom" HorizontalAlignment="Right" Content="VinnerAtack" Click="Button_Click"></Button>
        <Button x:Name="RSAtest" Height="40" Width="100" VerticalAlignment="Bottom" HorizontalAlignment="Left" Content="RSA" Click="RSAtest_Click" ></Button>
    </Grid>
</Window>
